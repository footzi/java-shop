CREATE TABLE goods (good_id serial PRIMARY KEY, name varchar(128) NOT NULL, price integer NOT NUll);

CREATE TABLE clients (client_id serial PRIMARY KEY, name varchar(128) NOT NULL, surname varchar(128), address varchar(128) NOT NULL, phone varchar(128) NOT NULL);
